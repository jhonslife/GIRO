{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Mercearias",
  "version": "0.1.0",
  "identifier": "com.arkheion.mercearias",
  "build": {
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Mercearias - PDV",
        "label": "main",
        "width": 1366,
        "height": 768,
        "minWidth": 1024,
        "minHeight": 600,
        "resizable": true,
        "center": true,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
